        "@types/cors": "^2.8.12",
        "@types/debug": "^4.1.7",
        "@types/express": "^4.17.14",
        "@types/jest": "^29.2.2",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/mongoose": "^5.11.97",
        "@types/morgan": "^1.9.3",
        "@types/node": "^18.11.9",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.42.1",
        "@typescript-eslint/parser": "^5.42.1",
        "bcryptjs": "^2.4.3",
        "eslint": "^8.25.0",
